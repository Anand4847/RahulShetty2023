<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests">

	<listeners>

		<listener class-name="TestCases.Listeners" />
	</listeners>


	<test thread-count="5" name="NewTestPass run">
		<classes>
			<class name="TestCases.NewTestPass" />
		</classes>
	</test> <!-- Test -->

	<test thread-count="5" name="NewTestFail run">
		<classes>
			<class name="TestCases.NewTestFail" />
		</classes>
	</test> <!-- Test -->

</suite> <!-- Suite -->
